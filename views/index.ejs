<!DOCTYPE html>
<html>
<head>
	<title>Chasing Shadows</title>
	<script
	  src="http://code.jquery.com/jquery-3.2.1.min.js"
	  integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
	  crossorigin="anonymous"></script>
	<script type="text/javascript" src="js/chasingshadows.js"></script>
	<link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
	<link href="https://fonts.googleapis.com/css?family=Spectral" rel="stylesheet">
	<link href="https://fonts.googleapis.com/css?family=Questrial" rel="stylesheet">
	<link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
	<script type="text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
	<script type="text/javascript" src="lib/masonry.pkgd.min.js"></script>
	<script type="text/javascript" src="lib/bigtext.js"></script>
	<script type="text/javascript" src="lib/imagesloaded.pkgd.min.js"></script>
	<link rel="stylesheet" type="text/css" href="css/chasingshadows.css">
	<script>document.write('<script src="http://' + (location.host || 'localhost').split(':')[0] + ':35729/livereload.js?snipver=1"></' + 'script>')</script>
</head>
<body>

<!--	COMMON 		-->

	<div class="container-fluid allDiv">
		<div class="row">
<!-- 
			<div class="hidden-desktop" id="miniNav">
				<span>This is where the menu will go!</span>
			</div>
 -->
		<!-- Nav 		-->
			<div id="mainNav" class="col col-lg-2 visible-lg navCol">
				<div>
					<div class="logo logoMain bigText">
						<h1 class="logoChasingMain">Chasing</h1>
						<h1 class="logoShadowsMain">Shadows</h1>
					</div>
					<div class="subLogo">
						<h2 class="logoPhotographyMain">Photography by</h2>
						<h2 class="logoLKBMain">Laura Kate Bemrose</h2>
					</div>
					<br>
					<br>
					<br>
					<br>
					<ul><h3 class="categoryLink">Nature</h3>
						<li hidden class="galLink galLinkMain" data-pics="14" data-gallery="Flowers" data-thumbClass="thumb4x4"><h3 id="navLinkFlowers">Hampton Court Flower Show</h3></li>
						<li hidden class="galLink galLinkMain" data-pics="13" data-gallery="Richmond" data-thumbClass="thumbEven"><h3 id="navLinkRichmond">Richmond Park</h3></li>
						<li hidden class="galLink galLinkMain" data-gallery="Raindrops"><h3 id="navLinkRaindrops">Raindrops</h3></li>
					</ul>
					<ul><h3 class="categoryLink">Travel</h3>
						<li hidden class="galLink galLinkMain" data-pics="22" data-gallery="Stockholm" data-thumbClass="thumbEven"><h3 id="navLinkStockholm">Stockholm</h3></li>
						<li hidden class="galLink galLinkMain" data-gallery="Cuba"><h3 id="navLinkCuba">Cuba</h3></li>
						<li hidden class="galLink galLinkMain" data-gallery="Paris"><h3 id="navLinkParis">Paris</h3></li>
					</ul>
					<ul><h3 class="categoryLink">Commissions</h3>
						<li hidden class="galLink galLinkMain" data-gallery="WeddingFlowers"><h3 id="navLinkWeddingFlowers">Wedding flowers</h3></li>
					</ul>
					<br>
					<br>
					<h3>About</h3>
				</div>
			</div>

			<div hidden id="musicNav" class="col col-lg-2 navCol">
				<div>
					<div class="logo logoMain bigText">
						<h1 class="logoChasingMain">Chasing</h1>
						<h1 class="logoShadowsMain">Shadows</h1>
					</div>
					<div class="subLogo">
						<h2 class="logoPhotographyMain">Photography by</h2>
						<h2 class="logoLKBMain">Laura Kate Bemrose</h2>
					</div>
					<br>
					<br>
					<br>
					<br>
					<ul><h3 class="categoryLink">Gigs</h3>
						<li hidden class="galLink galLinkMain" data-pics="14" data-gallery="Flowers" data-thumbClass="thumb4x4"><h3 id="navLinkA">A</h3></li>
						<li hidden class="galLink galLinkMain" data-pics="13" data-gallery="Richmond" data-thumbClass="thumbEven"><h3 id="navLinkB">B</h3></li>
					</ul>
					<ul><h3 class="categoryLink">Fashion</h3>
						<li hidden class="galLink galLinkMain" data-gallery="Stockholm"><h3 id="navLinkC">C</h3></li>
					</ul>
					<br>
					<br>
					<h3>About</h3>
				</div>
			</div>


			<div class="col col-lg-8 col-md-12 showPane">
				<div class="container-fluid">
					
				<!--	Cover image			-->
					<div id="showCover" class="showDiv row">
						<div class="col col-lg-12 showLandscapeImg">
							<img src="img\Cover.JPG">
						</div>
					</div>

					<div hidden id="musicCover" class="showDiv row">
						<div class="col col-lg-12 showLandscapeImg">
							<img src="img\CoverMusic.JPG">
						</div>
					</div>


				<!--	Thumbnails			-->
					<div hidden id="showThumbnails" class="showDiv row-fluid">
						<div class="col col-lg-3 col-md-6 col-sm-6 col-xs-12" id="thumbCol1"></div>
						<div class="col col-lg-3 col-md-6 col-sm-6 col-xs-12" id="thumbCol2"></div>
						<div class="col col-lg-3 col-md-6 col-sm-6 col-xs-12" id="thumbCol3"></div>
						<div class="col col-lg-3 col-md-6 col-sm-6 col-xs-12" id="thumbCol4"></div>
					</div>

				<!--	Zoomed images	-->
					<div hidden id="showZoomImg" class="showDiv row">
						<div class="col col-lg-12 showLandscapeImg">
							<img id="zoomImg" src="">
						</div>
					</div>

				<!--	Pic controls 		-->
					<div hidden class="picControls">
						<span class="controlSpan">
							<i class="fa fa-chevron-left picControl" id="prevBtn"></i>
							<i class="fa fa-times picControl" id="closePopUp"></i>
							<i class="fa fa-chevron-right picControl" id="nextBtn"></i>
						</span>
					</div>

				</div>
			</div>

		<!-- Icon bar 		-->
			<div class="col col-lg-2 visible-lg iconCol">
				<div class="footerIcons">
					<i class="fa fa-instagram" aria-hidden="true"></i>
					<i class="fa fa-envelope-o" aria-hidden="true"></i>
					<div class="flipContainer">
						<div id="flipBtn">
							<div class="flipDiv flipFront">
								<div class="flipTriangleFront"></div>
							</div>
							<div class="flipDiv flipBack">
								<div class="flipTriangleBack"></div>
							</div>
						</div>
					</div>
				</div>
			</div>

		</div>

	</div>
</body>
</html>




